@import "../star/starTemplate.wxss";

.movie_container {
  display: flex;
  flex-direction: column;
  width: 220rpx;
  margin: 5px;
}

.movie_image {
  width: 100%;
  height: 339rpx;
  margin-bottom: 10px;
}

.movie_title {
  font-size: 14px;
  font-family: Arial, Helvetica, sans-serif;
}

.movie_desc {
  display: flex;
  flex-direction: column;
  align-items: center;
}
